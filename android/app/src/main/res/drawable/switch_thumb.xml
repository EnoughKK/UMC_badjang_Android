<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item
    android:left="4dp"
    android:right="4dp"
    android:top="4dp"
    android:bottom="4dp">

    <shape
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:shape="oval">

        <size
            android:width="14dp"
            android:height="14dp" />

        <solid
            android:color="#FFFFFF" />

        <stroke
            android:width="1dp"
            android:color="#FFFFFF" />

    </shape>
</item>
</layer-list>